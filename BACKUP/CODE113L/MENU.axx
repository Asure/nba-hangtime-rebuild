GO_HARDWARE:
	move	@_man_date,a2,L
	move	a2,a1
	subk	1,a1
	movi	372,a0
	divu	a0,a1
	movi	372,a3
	mpyu	a1,a3
	sub	a3,a2
	addi	80,a1
	move	a1,@_year,1
	move	a2,a0
	movi	31,a1
	modu	a1,a0
	move	a0,@_day,1
	move	a2,a1
	subk	1,a1
	movi	31,a0
	divu	a0,a1
	addk	1,a1
	move	a1,@_month,1
	calla	CLR_SCRN		; Clear the screen
	movi	M_HARDWARE_INFO,a2	; Put up Title Box
	movi	ROBO_GREEN,a3
