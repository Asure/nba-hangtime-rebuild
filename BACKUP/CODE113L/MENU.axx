_hardware_check_sw:			; Wait for a button to be pressed
	SLEEP	10
	move	@_switch2_addr,a0,L
	move	*a0,a0,W
	move	@_switch_addr,a1,L
	move	*a1,a1,W
	sll	16,a0
	srl	16,a0
	sll	16,a1
	srl	16,a1
	and	a0,a1
	move	@_coin_addr,a0,L
	move	*a0,a0
	sll	16,a0
	or	a1,a0
	not	a0
	jrz	_hardware_check_sw
RET_HARDWARE:
	JSRP	FORCE_MAIN
	RETP





