	move	*a8(OCTRL),a14			;get current ctrl data
	andi	08fffH,a14			;clear bits (12-14)
	or	a1,a14				;set bits (12-14)
	move	a14,*a8(OCTRL)			;update 'ctrl' data

	PULL	a0,a10,a11
	dsj	a0,uvhkudlp
