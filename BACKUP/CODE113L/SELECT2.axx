START_PAL	equ	PDATA
CUR_PAL		equ	PDATA+20h
PAL_ADDR	equ	PDATA+40h
MORF_PAL	equ	PDATA+60h
SLEEP_TIME	equ	PDATA+80h

 SUBR	morf_pal


	move	a8,*a13(SLEEP_TIME)
	move	a10,*a13(START_PAL),L
	move	a10,*a13(CUR_PAL),L
	move	a11,*a13(MORF_PAL),L

blwpwait
	SLEEPK	1

	move	a9,a0
	calla	pal_find
	jrz	blwpwait
	srl	8,a0
	sll	8,a0
	move	a0,*a13(PAL_ADDR),L

	move	*a13(MORF_PAL),a1,L
	move	*a10,a2,L
	move	*a2+,a3		;num colours
blwploop
	move	*a2+,*a1+
	dsj	a3,blwploop

	move	*a13(SLEEP_TIME),a10
blwpdly1
	SLEEPK	1
	dsj	a10,blwpdly1
blwpnext_pal

	move	*a13(CUR_PAL),a10,L
	addi	20h,a10
	move	*a10,a0,L
	jrn	blwpok
	jrz	blwpnot_sleep

	move	a10,*a13(CUR_PAL),L
	move	a0,a10
	jruc	blwpdly1

blwpnot_sleep
	move	*a13(START_PAL),a10,L
blwpok
	move	a10,*a13(CUR_PAL),L

blwpagain
	move	*a13(SLEEP_TIME),a10
	srl	1,a10
	jrnz	blwpdly2
	movk	1,a10
blwpdly2
	SLEEPK	1
	dsj	a10,blwpdly2


	move	*a13(MORF_PAL),a1,L
	move	*a13(CUR_PAL),a10,L
	move	*a10,a2,L
	move	*a2+,a0			;num colours
	clr	a11
blwpmorfit
	move	*a1,a3			;CURRENT PALETTE
	movi	0111110000000000b,a4	;5 bits of red
	and	a3,a4
	movi	0000001111100000b,a5	;5 bits of green
	and	a3,a5
	movi	0000000000011111b,a6	;5 bits of blue
	and	a3,a6

	move	*a2+,a7			;DEST PALETTE
	movi	0111110000000000b,a8	;5 bits of red
	and	a7,a8
	movi	0000001111100000b,a9	;5 bits of green
	and	a7,a9
	movi	0000000000011111b,a10	;5 bits of blue
	and	a7,a10

	srl	10,a4
	srl	10,a8
	cmp	a4,a8			;a8-a4
	jreq	blwpredok
	jrlt	blwpdecr
	inc	a11
	inc	a4
	jruc	blwpredok
blwpdecr
	inc	a11
	dec	a4
blwpredok
	sll	10,a4

	srl	5,a5
	srl	5,a9
	cmp	a5,a9
	jreq	blwpgreenok
	jrlt	blwpdecg
	inc	a11
	inc	a5
	jruc	blwpgreenok
blwpdecg
	inc	a11
	dec	a5
blwpgreenok
	sll	5,a5

	cmp	a6,a10
	jreq	blwpblueok
	jrlt	blwpdecb
	inc	a11
	inc	a6
	jruc	blwpblueok
blwpdecb
	inc	a11
	dec	a6
blwpblueok

	or	a5,a4
	or	a6,a4
	move	a4,*a1+

	dsj	a0,blwpmorfit

	move	*a13(CUR_PAL),a0,L	;* palette list
	move	*a0,a0,L		;* palette
	move	*a0,a2			;num colours
	move	*a13(MORF_PAL),a0,L	;* palette
	move	*a13(PAL_ADDR),a1,L	;dest palette
	calla	pal_set

	move	a11,a11
	jrnz	blwpagain

	jruc	blwpnext_pal

	DIE




